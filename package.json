{
  "name": "liamhockley",
  "version": "1.0.0",
  "description": "The personal website for Liam Hockley",
  "main": "app.js",
  "author": "Liam Hockley",
  "license": "MIT",
  "scripts": {
    "fix:other": "npm run prettier --write",
    "fix:js": "npm run test:js --fix",
    "fix": "npm run fix:js && npm run fix:other",
    "prettier": "prettier \"**/*.{json,md,scss,yaml,yml}\"",
    "test:other": "npm run prettier --list-different",
    "test:js": "eslint --quiet --ignore-path .gitignore --ignore-path .prettierignore \"**/*.{js,jsx}\";exit 0",
    "test": "npm run test:other && npm run test:js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xdega/liamhockley.git"
  },
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "eslint": "5.10.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-prettier": "3.0.0",
    "prettier": "1.15.3"
  }
}
